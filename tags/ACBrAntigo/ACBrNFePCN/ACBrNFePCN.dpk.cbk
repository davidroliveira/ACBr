package ACBrNFePCN;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'ACBrNFePCN - (http://acbr.sourceforge.net/)'}
{$IMPLICITBUILD ON}

requires
  rtl,
  designide,
  Rave50VCL,
  xmlrtl,
  inet,
  soaprtl,
  {$IFDEF CLX}
    visualclx;
  {$ELSE}
    vcl ;
  {$ENDIF}

contains

  {$IFNDEF ACBrNFeOpenSSL}
     ACBrNFeMSXML2_TLB in 'ACBrNFeMSXML2_TLB.pas',
     ACBrNFeCAPICOM_TLB in 'ACBrNFeCAPICOM_TLB.pas',
     ACBrNFeNfeInutilizacaoU in 'ACBrNFeNfeInutilizacaoU.pas',
     ACBrNFeNfeCancelamentoU in 'ACBrNFeNfeCancelamentoU.pas',	 
     ACBrNFeNfeConsultaU in 'ACBrNFeNfeConsultaU.pas',
     ACBrNFeNfeRetRecepcaoU in 'ACBrNFeNfeRetRecepcaoU.pas',
	   ACBrNFeNfeRecepcaoU in 'ACBrNFeNfeRecepcaoU.pas',
     ACBrNFeNfeStatusServicoU in 'ACBrNFeNfeStatusServicoU.pas',
  {$ENDIF}	 
  ACBrNFe in 'ACBrNFe.pas',
  ACBrNFeUtil in 'ACBrNFeUtil.pas',
  ACBrNFeNotasFiscais in 'ACBrNFeNotasFiscais.pas',
  ACBrNFeDM in 'ACBrNFeDM.pas' {dmACBrNFe: TDataModule},
  ACBrNFeConfiguracoes in 'ACBrNFeConfiguracoes.pas',
  ACBrNFeWebServices in 'ACBrNFeWebServices.pas',
  ACBrNFeReg in 'ACBrNFeReg.pas',
  pcnValidador in 'pcnValidador.pas',
  pcnAtuCadEmiDFe in 'pcnAtuCadEmiDFe.pas',
  pcnAuxiliar in 'pcnAuxiliar.pas',
  pcnCabecalho in 'pcnCabecalho.pas',
  pcnCadEmiDFe in 'pcnCadEmiDFe.pas',
  pcnCancNFe in 'pcnCancNFe.pas',
  pcnConsCad in 'pcnConsCad.pas',
  pcnConsReciNFe in 'pcnConsReciNFe.pas',
  pcnConsSitNFe in 'pcnConsSitNFe.pas',
  pcnConsStatServ in 'pcnConsStatServ.pas',
  pcnConversao in 'pcnConversao.pas',
  pcnGerador in 'pcnGerador.pas',
  pcnInutNFe in 'pcnInutNFe.pas',
  pcnLeitor in 'pcnLeitor.pas',
  pcnModeloNFe in 'pcnModeloNFe.pas',
  pcnNFe in 'pcnNFe.pas',
  pcnNFeR in 'pcnNFeR.pas',
  pcnNFeW in 'pcnNFeW.pas',
  pcnRetAtuCadEmiDFe in 'pcnRetAtuCadEmiDFe.pas',
  pcnRetCancNFe in 'pcnRetCancNFe.pas',
  pcnRetConsCad in 'pcnRetConsCad.pas',
  pcnRetConsReciNFe in 'pcnRetConsReciNFe.pas',
  pcnRetConsSitNFe in 'pcnRetConsSitNFe.pas',
  pcnRetConsStatServ in 'pcnRetConsStatServ.pas',
  pcnRetEnvNFe in 'pcnRetEnvNFe.pas',
  pcnRetInutNFe in 'pcnRetInutNFe.pas',
  pcnLayoutTXT in 'pcnLayoutTXT.pas',
  pcnSignature in 'pcnSignature.pas'  ;

end.
